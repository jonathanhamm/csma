#Node Declarations
nodes = {
            a = node("a"),
            b = node("b"),
            c = node("c"),
            d = node("d"),
            e = node("e"),
            f = node("f"),
            g = node("g"),
            h = node("h"),
            i = node("i"),
            j = node("j")
}

nodes.a.send(nodes, "Hello Son", repeat=true, period=0.3)